{
  "browserslist": {
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ],
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ]
  },
  "dependencies": {
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-scripts": "^3.2.0",
    "typescript": "3.7.2"
  },
  "devDependencies": {
    "@types/dotenv": "^8.2.0",
    "@types/jest": "^24.0.23",
    "@types/node": "^12.12.14",
    "@types/reach__router": "^1.2.6",
    "@types/react": "^16.9.13",
    "@types/react-dom": "^16.9.4",
    "dotenv": "^8.2.0",
    "husky": "^3.1.0",
    "jest-dom": "^4.0.0",
    "jest-emotion": "^10.0.17",
    "lint-staged": "^9.5.0",
    "node-fetch": "^2.6.0",
    "prettier": "^1.19.1",
    "pretty-quick": "^2.0.1"
  },
  "engines": {
    "node": "10.15.1",
    "npm": "6.4.1"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run test:unit:coverage"
    }
  },
  "lint-staged": {
    "*.{ts, tsx}": [
      "npm run lint",
      "npm run test:unit",
      "pretty-quick --staged",
      "git add"
    ]
  },
  "name": "vida-suprema-frontend",
  "private": true,
  "scripts": {
    "build": "react-scripts build",
    "coverage": "react-scripts test --coverage",
    "deploy": "aws s3 sync build/ s3://vida-suprema-frontend",
    "eject": "react-scripts eject",
    "lint": "eslint src/ --ext .ts,.tsx ",
    "start": "react-scripts start",
    "test": "react-scripts test",
    "test:unit": "CI=true react-scripts test --env=jsdom --passWithNoTests",
    "test:unit:coverage": "CI=true react-scripts test --env=jsdom --passWithNoTests --coverage --silent"
  },
  "version": "0.0.0"
}
